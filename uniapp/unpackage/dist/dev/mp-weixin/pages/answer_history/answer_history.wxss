
.answer-wrap {
	width: 94%;
	margin: 0 auto;
}
.answer-item {
	height:140rpx;
	padding-top: 20rpx;
	border-bottom: 2rpx solid #F0F0F0;
}
.answer-item-one {
	font-size: 32rpx;
	position: relative;
}
.answer-item-two {
	margin-top: 20rpx;
	font-size: 24rpx;
	color: #969696;
}
.answer-item-one-hms {
	display: inline-block;
